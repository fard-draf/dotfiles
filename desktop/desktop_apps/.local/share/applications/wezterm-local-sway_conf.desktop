

[Desktop Entry]
Version=1.0
Type=Application
Name=SwayConf
Comment=Lance les confs liées à Sway
Exec=env WEZTERM_SOCKET=/tmp/wezterm-local flatpak run org.wezfurlong.wezterm start --class zellij -- /home/fard/.cargo/bin/zellij --layout sway_conf
# L'identifiant pour GNOME, qui doit correspondre à l'option --class
StartupWMClass=zellij
Icon=/home/fard/Pictures/Icones/settings.png
Terminal=false
Categories=System;TerminalEmulator;
